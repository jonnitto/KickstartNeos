prototype(Queo.SitePackage:Content.Headline) < prototype(Neos.Neos:ContentComponent) {
    title = Neos.Neos:Editable {
        property = 'title'
    }
    tagName = ${q(node).property('tagName') || 'h2'}

    renderer = Queo.SitePackage:Component.Atom.Headline {
        @apply.props = ${props}
    }
}
